<root>
    <styles>
 		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
        <include src="file://{resources}/styles/custom_game/multiteam_end_screen.css" />
    </styles>
	<Panel class="PlayerRow">
		<Panel id="PlayerRowContainer">
			<Panel id="Avatar" class="ScoreCol_Avatar ScoreboardPanel">
            	<DOTAAvatarImage id="AvatarImage" steamid="local" />
            	<Image id="BorderImage" hittest="false" class="HiddenBorder" />
        	</Panel>
			<Panel id="PlayerDetailsContainer" class="ScoreCol_PlayerDetails ScoreboardPanel">
				<Panel id="PlayerAndHeroName">
					<Label id="PlayerName" text="AAAAA" />
					<Label id="HeroNameAndDescription" text="#custom_scoreboard_hero_name_and_desc" />
				</Panel>
			</Panel>
			<Panel id="ScoreContainer" class="ScoreCol_Score ScoreboardPanel">
				<Label id="Score" text="0" />
			</Panel>
			<Panel id="GoldContainer" class="ScoreCol_Gold ScoreboardPanel">
				<Label id="Networth" text="0" />
			</Panel>
			<Panel id="WaveContainer" class="ScoreCol_Wave ScoreboardPanel">
				<Label id="Wave" text="0" />
			</Panel>
			<Panel id="KillsContainer" class="ScoreCol_Kills ScoreboardPanel">
				<Label id="KillsEnd" class="Kills_Standard" text="0" />
				<Image id="OSfrog" class="hide"/>
			</Panel>
			<Panel id="ElementsContainer" class="ScoreCol_Elements ScoreboardPanel">
				<Panel id="ElemSubContainer">
					<Label id="HasRandomed" class='hide' text=""/>
					<Panel id="light" class="ElemImageAndLevel">
						<Image class="ElementImage" src="file://{images}/spellicons/elements/light.png"/>
						<Label id="light_level" text=""/>
					</Panel>
					<Panel id="dark" class="ElemImageAndLevel">
						<Image class="ElementImage" src="file://{images}/spellicons/elements/dark.png"/>
						<Label id="dark_level" text=""/>
					</Panel>
					<Panel id="water" class="ElemImageAndLevel">
						<Image class="ElementImage" src="file://{images}/spellicons/elements/water.png"/>
						<Label id="water_level" text=""/>
					</Panel>
					<Panel id="fire" class="ElemImageAndLevel">
						<Image class="ElementImage" src="file://{images}/spellicons/elements/fire.png"/>
						<Label id="fire_level" text=""/>
					</Panel>
					<Panel id="nature" class="ElemImageAndLevel">
						<Image class="ElementImage" src="file://{images}/spellicons/elements/nature.png"/>	
						<Label id="nature_level" text=""/>
					</Panel>
					<Panel id="earth" class="ElemImageAndLevel">
						<Image class="ElementImage" src="file://{images}/spellicons/elements/earth.png"/>
						<Label id="earth_level" text=""/>
					</Panel>
					<Panel id="pure" class="ElemImageAndLevel">
						<Image class="ElementImage" src="file://{images}/spellicons/elements/pure.png"/>
						<Label id="pure_level" text=""/>
					</Panel>
				</Panel>
			</Panel>
		</Panel>
	</Panel>
</root>
